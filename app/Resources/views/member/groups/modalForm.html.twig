<div class="modal fade" id="memberGroupModalLabel" tabindex="-1" role="dialog" aria-labelledby="memberGroupModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="memberGroup">
        <div class="modal-content">
            {{ form_start(form, {'action': path('member_group_save_json', {'id': memberGroup.id}), attr:{
                'id': 'member_group_form_type'}
            }) }}
            <div class="modal-header">
                <h5 class="modal-title" id="documentModalLabel">{% if memberGroup.id > 0 %}{{ 'member.groups.edit.title'|trans }}{% else %}{{ 'member.groups.add.title'|trans }}{% endif %}</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                {% include 'member/groups/formContent.html.twig' %}
            </div>
            <div class="modal-footer">
                <button type="button" id="btnCancelMemberGroup" class="btn btn-secondary" data-dismiss="modal">{{ 'app.common.cancelBtn'|trans }}</button>
                <button type="submit" id="btnSaveMemberGroup" class="btn btn-primary">{{ 'app.common.form.submit.save'|trans }}</button>
            </div>
            {{ form_end(form) }}
        </div>
    </div>
</div>